		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/vite": "^4.1.16",
		"@tanstack/react-devtools": "^0.7.11",
		"@tanstack/react-form": "^1.23.8",
		"@tanstack/react-router": "^1.133.36",
