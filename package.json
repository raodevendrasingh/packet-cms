{
	"name": "packet-cms",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run fix && vite dev --port 3000",
		"build": "bun run fix && vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"typecheck": "bun x tsc --noEmit",
		"check": "bun x ultracite@latest check",
		"fix": "bun x ultracite@latest fix",
		"deploy": "bun run build && wrangler deploy",
		"preview": "bun run build && vite preview",
		"cf-typegen": "wrangler types"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.13.17",
		"@hugeicons/core-free-icons": "^1.2.1",
		"@hugeicons/react": "^1.1.1",
		"@radix-ui/react-slot": "^1.2.3",
		"@tailwindcss/vite": "^4.1.16",
		"@tanstack/react-devtools": "^0.7.11",
		"@tanstack/react-router": "^1.133.36",
		"@tanstack/react-router-devtools": "^1.133.36",
		"@tanstack/react-router-ssr-query": "^1.133.36",
		"@tanstack/react-start": "^1.134.0",
		"@tanstack/router-plugin": "^1.133.36",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.16",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.9.2",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.1.0",
		"husky": "^9.1.7",
		"jsdom": "^27.0.1",
		"lint-staged": "^16.2.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"ultracite": "6.1.0",
		"vite": "^7.1.12",
		"vitest": "^4.0.5",
		"web-vitals": "^5.1.0",
		"wrangler": "^4.45.2"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
			"bun x ultracite fix"
		]
	}
