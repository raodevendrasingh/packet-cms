		"clsx": "^2.1.1",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.44.7",
		"next-themes": "^0.4.6",
		"postgres": "^3.4.7",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
