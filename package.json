		"drizzle-kit": "^0.31.6",
		"husky": "^9.1.7",
		"jsdom": "^27.0.1",
		"lint-staged": "^16.2.6",
		"tsx": "^4.20.6",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"ultracite": "6.1.0",
		"vite": "^7.1.12",
		"vitest": "^4.0.5",
		"web-vitals": "^5.1.0",
		"wrangler": "^4.45.2"
