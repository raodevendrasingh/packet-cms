{
	"name": "packet-cms",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run fix && vite dev --port 3000",
		"build": "bun run fix && vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"typecheck": "bun x tsc --noEmit",
		"check": "bun x ultracite@latest check",
		"fix": "bun x ultracite@latest fix",
		"deploy": "bun run build && wrangler deploy",
		"preview": "bun run build && vite preview",
		"cf-typegen": "wrangler types",
		"db:setup": "bash scripts/setup-db.sh",
		"db:push": "bun x drizzle-kit push",
		"db:generate": "bun x drizzle-kit generate",
		"db:migrate": "bun x drizzle-kit migrate",
		"db:studio": "bun x drizzle-kit studio"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.13.17",
		"@hugeicons/core-free-icons": "^1.2.1",
		"@hugeicons/react": "^1.1.1",
